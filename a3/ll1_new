program ::= var_declarations statements
program ::= statements
var_declarations ::= var_declaration var_declarations'
var_declarations' ::= var_declaration var_declarations'
var_declarations' ::= ''
var_declaration ::= INT declaration_list SEMI
declaration_list ::= declaration declaration_list'
declaration_list' ::= COMMA declaration declaration_list'
declaration_list' ::= ''
declaration ::= IDENTIFIER declaration'
code_block ::= statement
code_block ::= LBRACE statements RBRACE
statements ::= statement statements'
statements' ::= statement statements'
statements' ::= ''
statement ::= assign_statement SEMI
statement ::= control_statement
statement ::= read_write_statement SEMI
statement ::= SEMI
control_statement ::= if_statement
control_statement ::= while_statement
control_statement ::= do_while_statement SEMI
control_statement ::= return_statement SEMI
read_write_statement ::= read_statement
read_write_statement ::= write_statement
assign_statement ::= IDENTIFIER assign_statement'
if_statement ::= if_stmt if_statement'
if_stmt ::= IF LPAR exp RPAR code_block
if_statement' ::= ELSE code_block
if_statement' ::= ''
while_statement ::= WHILE LPAR exp RPAR code_block
do_while_statement ::= DO code_block WHILE LPAR exp RPAR
return_statement ::= RETURN
read_statement ::= READ LPAR IDENTIFIER RPAR
write_statement ::= WRITE LPAR exp RPAR
declaration' ::= ASSIGN NUMBER
declaration' ::= LSQUARE NUMBER RSQUARE
declaration' ::= ''
assign_statement' ::= LSQUARE exp RSQUARE ASSIGN exp
assign_statement' ::= ASSIGN exp
exp ::= IDENTIFIER exp' exp3
exp ::= INT_NUM exp3
exp ::= NOT_OP exp exp3
exp ::= LPAR exp RPAR exp3
exp ::= MINUS exp exp3
exp' ::= ''
exp' ::= LSQUARE exp RSQUARE
exp2 ::= AND_OP exp
exp2 ::= OR_OP exp
exp2 ::= PLUS exp
exp2 ::= MINUS exp
exp2 ::= MUL_OP exp
exp2 ::= DIV_OP exp
exp2 ::= LT exp
exp2 ::= GT exp
exp2 ::= EQ exp
exp2 ::= NOTEQ exp
exp2 ::= LTEQ exp
exp2 ::= GTEQ exp
exp2 ::= SHL_OP exp
exp2 ::= SHR_OP exp
exp2 ::= ANDAND exp
exp2 ::= OROR exp
exp3 ::= exp2 exp3
exp3 ::= ''




